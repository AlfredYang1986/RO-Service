# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# company 用户公司和部门信息
# 基本接口，没有权限判断，暂时开放，后面上线屏蔽
POST    /company/push                           controllers.CompanyController.pushCompany
POST    /company/update                         controllers.CompanyController.updateCompany
POST    /company/drop                           controllers.CompanyController.dropCompany
POST    /company/query                          controllers.CompanyController.queryCompany
POST    /company/lst                            controllers.CompanyController.listCompany
POST    /company/page                           controllers.CompanyController.pageCompany

# product 公司产品信息
# 基本接口，没有权限判断，暂时开放，后面上线屏蔽
POST    /product/push                           controllers.ProductController.pushProduct
POST    /product/update                         controllers.ProductController.updateProduct
POST    /product/drop                           controllers.ProductController.dropProduct
POST    /product/query                          controllers.ProductController.queryProduct
POST    /product/lst                            controllers.ProductController.listProducts
POST    /product/page                           controllers.ProductController.pageProducts

# auth 用户授权 End Point (详情参考概要设计，@Alfred)
POST    /auth/password                          controllers.AuthController.authWithPassword
POST    /auth/changepwd                         controllers.AuthController.authChangePwd
POST    /auth/logout                            controllers.AuthController.authLogout
POST    /auth/update                            controllers.AuthController.authUpdate
POST    /auth/dropUser                          controllers.AuthController.authDropUser
POST    /auth/pushUser                          controllers.AuthController.authPushUser
POST    /auth/queryUser                         controllers.AuthController.queryUser
POST    /auth/checkAuthToken                    controllers.AuthController.authTokenCheckTest
POST    /auth/lst                               controllers.AuthController.queryMultipleUser
POST    /auth/page                              controllers.AuthController.pageUser

# department 部门信息
POST    /department/push                        controllers.DepartmentController.pushDepartment
POST    /department/update                      controllers.DepartmentController.updateDepartment
POST    /department/drop                        controllers.DepartmentController.dropDepartment
POST    /department/query                       controllers.DepartmentController.queryDepartment
POST    /department/lst                         controllers.DepartmentController.listDepartments
POST    /department/page                        controllers.DepartmentController.pageDepartments

# channel 产品渠道信息
POST    /channel/push                           controllers.ChannelController.pushChannel
POST    /channel/update                         controllers.ChannelController.updateChannel
POST    /channel/drop                           controllers.ChannelController.dropChannel
POST    /channel/query                          controllers.ChannelController.queryChannel
POST    /channel/lst                            controllers.ChannelController.listChannels
POST    /channel/page                           controllers.ChannelController.pageChannels

# data 数据管理
POST    /data/import                            controllers.DataController.importData


# common 通用
